{
  "manifest_version": 3,
  "name": "Quote Collector",
  "version": "1.0.0",
  "description": "Save quotes to Google Sheets",
  "key": "",

  "permissions": [
    "identity",
    "activeTab",
    "storage"
  ],

  "action": {
    "default_popup": "popup-working.html",
    "default_title": "Quote Collector",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png", 
      "128": "icon-128.png"
    }
  },

  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}